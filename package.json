{
  "name": "@gitpad/gmodal",
  "version": "0.0.1",
  "description": "A modal layer for react",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && webpack --config ./dev/webpack.config.js",
    "build:dev": "npm run clean && webpack --config ./dev/webpack.development.js",
    "example": "webpack --config ./dev/webpack.example.js",
    "start": "node ./dev/serve.js",
    "prepublish": "",
    "pub": "npm publish --access public",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitpadtech/gmodal.git"
  },
  "author": "yeluoqiuzhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gitpadtech/gmodal/issues"
  },
  "homepage": "https://github.com/gitpadtech/gmodal#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "raf": "^3.4.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^0.3.1",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^4.0.0-beta.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/dev/jest/polyfills.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  }
}
